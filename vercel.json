{
  "routes": [
    {"src": "/ads\\.txt$", "dest": "/ads.txt"},
    {"src": "/(.*)\\.json$", "dest": "/$1.json"},
    {"src": "/(.*)\\.js$", "dest": "/$1.js"},
    {"src": "/(.*)\\.css$", "dest": "/$1.css"},
    {"src": "/public/(.*)", "dest": "/public/$1"},
    {"src": "/(.*)", "dest": "/index.html"}
  ]
}
